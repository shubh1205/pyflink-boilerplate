[
  {
  "PropertyGroupId": "kinesis.analytics.flink.run.options",
  "PropertyMap": {
    "python": "GettingStarted/getting-started.py",
    "jarfile": "GettingStarted/javalib/flink-sql-connector-kinesis-1.16.0.jar"
   }
  },
  {
    "PropertyGroupId": "consumer.config.0",
    "PropertyMap": {
      "input.stream.name": "input-stream",
      "flink.stream.initpos": "LATEST",
      "aws.region": "us-east-1",
      "aws.endpoint": "http://host.docker.internal:4566"
    }
  },
  {
    "PropertyGroupId": "producer.config.0",
    "PropertyMap": {
      "output.stream.name": "output-stream",
      "shard.count": "1",
      "aws.region": "us-east-1",
      "aws.endpoint": "http://host.docker.internal:4566"
    }
  }
]